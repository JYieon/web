<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script>
        const viewMember = () => {
            fetch("/quiz_members", {method : "get"})
            .then(res => res.json())
            .then(data => {
                console.log(data)
                let msg = ""
                data.forEach(m => {
                    msg += m.id + ", " + m.name + "<br>"
                });
                const content = document.getElementById("content")
                    content.insertAdjacentHTML("beforeend", msg) //태그 뒤에 붙여놓음
            })
        }
    </script>
</head>
<body>
    <div id="content"></div>
    <hr>
    <button onclick="viewMember()">더보기</button>
</body>
</html>